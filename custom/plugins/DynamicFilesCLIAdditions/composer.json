{
    "name": "dynamicfiles/shopware-cli-additions",
    "description": "Shopware Plugin for additional CLI commands",
    "type": "shopware-platform-plugin",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Danny Althoff",
            "email": "fibrefox@dynamicfiles.de.de",
            "homepage": "https://www.dynamicfiles.de",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
          "DynamicFiles\\Shopware\\CLIAdditions\\": "src/"
        }
    },
    "require": {
        "shopware/core": ">=6.4"
    },
    "extra": {
        "shopware-plugin-class": "DynamicFiles\\Shopware\\CLIAdditions\\DFCLIAdditions",
        "label": {
          "de-DE": "Shopware CLI Zusatzbefehle",
          "en-GB": "Shopware CLI additional commands"
        },
        "description": {
            "de-DE": "Ergänzung nützlicher Shopware CLI Befehle für die Steuerung von Funktionen, die nicht im offiziellen Shop vorhanden sind",
            "en-GB": "Supplemental useful Shopware CLI commands for controlling features, which are not existing in the official shop"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        },
        "process-timeout": 0
    }
}
